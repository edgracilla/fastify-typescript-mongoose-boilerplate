{
  "name": "fastify-typescript-mongoose-boilerplate",
  "version": "0.0.1",
  "description": "Fastify Typescript Mongoose Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "pm2 start pm2.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon",
    "test": "cross-env NODE_ENV=test jest --colors --verbose",
    "test:one:cov": "cross-env NODE_ENV=test jest --colors --verbose --coverage=true",
    "test:all:debug": "cross-env NODE_ENV=test jest -i --colors --verbose --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edgracilla/fastify-typescript-mongoose-boilerplate.git"
  },
  "keywords": [
    "node",
    "node.js",
    "boilerplate",
    "fastify",
    "typescript",
    "generator",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "jest",
    "docker",
    "eslint"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edgracilla/fastify-typescript-mongoose-boilerplate/issues"
  },
  "homepage": "https://github.com/edgracilla/fastify-typescript-mongoose-boilerplate#readme",
  "dependencies": {
    "@fastify/cors": "^8.0.0",
    "@fastify/formbody": "^7.0.1",
    "@fastify/helmet": "^9.1.0",
    "@fastify/redis": "^6.0.0",
    "deep-object-diff": "^1.1.7",
    "dotenv": "^16.0.1",
    "fastify": "^4.5.2",
    "globby": "^11.1.0",
    "ioredis-mock": "^8.2.2",
    "lodash": "^4.17.21",
    "mongo-querystring": "^4.1.1",
    "mongoose": "^6.4.2",
    "nanoid": "^3.3.4",
    "pino-pretty": "^8.1.0",
    "pm2": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.182",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "jest": "^28.1.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
}
